<!DOCTYPE html>
<html class="scroll-smooth bg-white antialiased" lang="{{ app.request.locale|split('_')[0] }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% include "partial/header.html.twig" %}
    <script defer type="text/javascript" src="/klaro/config.js"></script>
    <script
        defer
        data-config="klaroConfig"
        type="application/javascript"
        src="/klaro/klaro.js">
    </script>
    <link href="/klaro/klaro.min.css" rel="stylesheet"/>
    <link href="/klaro/klaro-custom.css" rel="stylesheet"/>
    {% block style %}
        {{ encore_entry_link_tags('style') }}
    {% endblock %}
    {% include 'partial/analytics.html.twig' %}
</head>
<body>

<div class="mx-auto max-w-7xl py-24 sm:px-6 sm:py-32 lg:px-8">
    {% include 'includes/tailwind/navigation.html.twig' %}
    <main id="content">
      {% block content %}{% endblock %}
    </main>
</div>
{% include 'includes/tailwind/snippets/footer.html.twig' %}
</body>
{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</html>
